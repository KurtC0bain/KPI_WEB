@page
@model PishchukM.Pages.MainModel

@{
    ViewData["Title"] = "Main";
}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/slider.css">
    <link rel="stylesheet" href="~/css/jquery-ui-2.css">
    <link rel="stylesheet" href="~/css/social.css">
    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/jquery.jdigiclock.css">
    <link rel="stylesheet" href="~/css/currency.css">
    <link rel='stylesheet' href='~/unitegallery/themes/default/ug-theme-default.css' type='text/css' /> 
    <link rel='stylesheet' href='~/unitegallery/css/unite-gallery.css' type='text/css' /> 
    <title>Server</title>
</head>
    <body>
        <div>
            <h1>Accordion</h1>
            <div class="accordion">
                <div class="accordion__item">
                        <div class="accordion__title">
                            <div class="accordion__arrow"><span class="accordion__arrow-item ">+</span></div> 
                            <span class="accordion__title-text">Veni</span>
                        </div>
                        <div class="accordion__content">
                            Прийшов
                        </div>
                </div>
                  <div class="accordion__item">
                        <div class="accordion__title">
                            <div class="accordion__arrow"><span class="accordion__arrow-item ">+</span></div> 
                            <span class="accordion__title-text">Vidi</span>
                        </div>
                        <div class="accordion__content">
                            Побачив
                        </div>
                </div>
                  <div class="accordion__item">
                        <div class="accordion__title">
                            <div class="accordion__arrow"><span class="accordion__arrow-item ">+</span></div> 
                            <span class="accordion__title-text">Vici</span>
                        </div>
                        <div class="accordion__content">
                            Переміг
                        </div>
                </div>
            </div>
            </div>
            <br><br><br>

            <h1>Slider</h1>
            <div id="polzunok"></div>
            <b style="font-size: 20px;"><br>Value: <span style="color:green; font-size: 30px;" id="result-polzunok"></span></b>

            <br><br><br>

            <h1>Clock</h1>
            <div id="myclock"></div>

            <h1>Weather&Clock</h1>
            <div id="digiclock"></div>

            <h1>Currency</h1>
            <br>
            <a href="//finance.i.ua/" target="_blank"><img src="//f.i.ua/fp1_b15_c0_l0.png" border="0" alt="Курс долара"></a>


               <form method="post">
                <fieldset>
                    <legend>Register</legend>
                    <div>
                        <label for="CompanyName">First Name:</label>
                        <input type="text" name="FirstName" value="" />
                        @Html.ValidationMessage("firstname")
                    </div>
                    <div>
                        <label for="ContactName">Last Name:</label>
                        <input type="text" name="LastName" value="" />
                        @Html.ValidationMessage("lastname")
                    </div>
                    <div>
                        <label for="Employees">Password:</label>
                        <input type="text" name="Password" value="" />
                        @Html.ValidationMessage("password")
                    </div>
                    <div>
                        <label>&nbsp;</label>
                        <input type="submit" value="Submit" class="submit" />
                    </div>
                </fieldset>
            </form>



            <script src="~/js/jquery-3.6.0.min.js"></script>
            <script src="~/js/jquery-ui.js"></script>
            <script src="~/js/jquery.prettySocial.js"></script> 
            <script src="~/js/jquery.thooClock.js"></script>
            <script src="~/js/jquery.jdigiclock.js"></script>
            <script src="~/js/jquery.inline-currency.js"></script>
            <script src="~/js/script.js"></script>
            <script type='text/javascript' src='http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js'></script>
            <script type='text/javascript' src='~/unitegallery/js/unitegallery.min.js'></script>     
            <script type='text/javascript' src='~/unitegallery/themes/default/ug-theme-default.js'></script> 
            <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>


        @{
            <br>
            <br>
             <text>
                      You entered: <br />
                      First Name: @Model.firstname <br />
                      Last Name:  @Model.lastname <br />
                      Password:   @Model.password <br />
             </text>
        }
  

    </body>
</html>